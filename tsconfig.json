{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "src",
    "paths": {
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/context/*": [
        "context/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/card/*": [
        "app/card/*"
      ],
      "@/photobooth/*": [
        "app/photobooth/*"
      ],
      "@/valentineday/*": [
        "app/valentineday/*"
      ],
      "@/sugaverse/*": [
        "app/sugaverse/*"
      ],
      "@/hobipalooza/*": [
        "app/hobipalooza/*"
      ],
      "@/vpassport/*": [
        "app/vpassport/*"
      ],
      "@/hopeisback/*": [
        "app/hopeisback/*"
      ],
      "@/virthday/*": [
        "app/virthday/*"
      ],
      "@/lovenotes/*": [
        "app/lovenotes/*"
      ],
      "@/seokjin/*": [
        "app/seokjin/*"
      ],
      "@/rps/*": [
        "app/rps/*"
      ],
      "@/tictactoe/*": [
        "app/tictactoe/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/festa/*": [
        "app/festa/*"
      ],
      "@/app/*": [
        "app/*"
      ],
      "@/services/*": [
        "services/*"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}